{
  "name": "gwent-cards",
  "version": "2.3.0",
  "description": "Discord Bot designed for Gwent cards display",
  "main": "bot.js",
  "scripts": {
    "build":
      "git submodule update --init --recursive && git submodule foreach git pull origin master && yarn --cwd Gwent-Cards-Data && yarn --cwd Gwent-Cards-Data start",
    "start": "babel-node bot.js",
    "test": "nodemon bot.js --exec babel-node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PabloSzx/Gwent-Cards.git"
  },
  "keywords": ["discord.js", "discord", "node", "heroku", "gwent", "gwent.io"],
  "author": "PabloSzx",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PabloSzx/Gwent-Cards/issues"
  },
  "homepage": "https://pabloszx.github.io/Gwent-Cards/",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "discord.js": "^11.1.0",
    "fast-levenshtein": "^2.0.6",
    "firebase": "^4.1.3",
    "gwent-pro-ladder-lib": "^1.0.3",
    "lodash": "^4.17.4",
    "nodemon": "^1.12.1",
    "npm": "^5.3.0",
    "object.values": "^1.0.4"
  },
  "devDependencies": {
    "prettier": "^1.5.3"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
