{
  "name": "gwent-cards",
  "version": "2.3.0",
  "description": "Discord Bot designed for Gwent cards display",
  "main": "bot.js",
  "scripts": {
    "heroku-postbuild": "echo Build Done!",
    "fetch": "git submodule update --init --recursive && git submodule foreach git pull origin master",
    "build": "yarn --cwd Gwent-Cards-Data && yarn --cwd Gwent-Cards-Data start",
    "start": "babel-node bot.js",
    "dev": "nodemon bot.js --exec babel-node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PabloSzx/Gwent-Cards.git"
  },
  "keywords": [
    "discord.js",
    "discord",
    "node",
    "heroku",
    "gwent",
    "gwent.io"
  ],
  "author": "PabloSzx",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PabloSzx/Gwent-Cards/issues"
  },
  "homepage": "https://pabloszx.github.io/Gwent-Cards/",
  "dependencies": {
    "axios": "^0.19.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "discord.js": "^11.1.0",
    "fast-levenshtein": "^2.0.6",
    "firebase": "^6.3.3",
    "gwent-pro-ladder-lib": "^1.0.3",
    "lodash": "^4.17.15",
    "object.values": "^1.1.0"
  },
  "devDependencies": {
    "prettier": "^1.18.2",
    "nodemon": "^1.19.1"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
